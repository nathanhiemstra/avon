<div class="table-responsive-rows-and-cols table-contact-list">
  <table class="table table-striped table-last-col-right">
    <thead>
      <tr>
        <th class="t-checkbox"><span class="d-none">Select</span></th>
        <th class="t-first-name">First Name</th>
        <th class="t-last-name">Last Name</th>
        <th class="t-phone">Phone</th>
        <th class="t-email">Email</th>
      {% if contactType == 'customer' %}
        <!-- START - For "Customer List" only -->
        <th class="t-online-customer">Online Customer</th>
        <!-- END - For "Customer List" only -->
      {% endif %}
      {% if contactType == 'team' %}
        <!-- START - For "Team List" only -->
        <th class="t-upline">Upline</th>
        <th class="t-gen">Gen.</th>
        <!-- END - For "Team List" only -->
      {% endif %}
        <th class="t-actions "><span class="d-none">Actions</span></th>
      </tr>
    </thead>
    <tbody>
      {% include '_includes/modules/contacts/contacts-list/contacts-list-tr.html.twig' with { isOnlineCustomer: true } %}
      {% include '_includes/modules/contacts/contacts-list/contacts-list-tr.html.twig' with { isOnlineCustomer: true } %}
      {% include '_includes/modules/contacts/contacts-list/contacts-list-tr.html.twig' with { isOnlineCustomer: false } %}
      {% include '_includes/modules/contacts/contacts-list/contacts-list-tr.html.twig' with { isOnlineCustomer: false } %}
      {% include '_includes/modules/contacts/contacts-list/contacts-list-tr.html.twig' with { isOnlineCustomer: false } %}
      {% include '_includes/modules/contacts/contacts-list/contacts-list-tr.html.twig' with { isOnlineCustomer: false } %}
      {% include '_includes/modules/contacts/contacts-list/contacts-list-tr.html.twig' with { isOnlineCustomer: false } %}
    </tbody>
  </table>
</div>
