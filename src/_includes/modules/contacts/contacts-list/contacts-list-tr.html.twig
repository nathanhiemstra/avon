<tr class="t-has-col-start">

  <td class="t-checkbox t-row-1 t-col-start pr-0">
    {% include "_includes/elements/forms/checkbox.html.twig" with { checked: false, classes: 'input-sm ', wrapLabel: true } %}
  </td>

  <td class="t-first-name t-row-1 t-inline t-col-sibling-left">
    <div class="truncate">
      <a href="#" class="link-secondary link-underline">Firstname{{ longFirstName }}</a>
    </div>
  </td>

  <td class="t-last-name t-row-1 t-inline">
    <div class="truncate">
      <a href="#" class="link-secondary link-underline">Lastname{{ longLastName }}</a>
    </div>
  </td>

  {% if contactType == 'team' %}
  <!-- START - For "Team List" only -->


   <td class="t-upline t-row-2 t-block t-col-sibling-left">
    <div class="truncate">
      <span class="t-key">Upline: </span>Contact Name{{ longContactName }}
    </div>
  </td>

  <td class="t-gen t-row-2 t-block t-col-sibling-left">
    <div class="truncate">
      <span class="t-key">Gen: </span>00000
    </div>
  </td>
  <!-- END - For "Team List" only -->
{% endif %}


  <td class="t-phone t-row-3 t-block t-col-sibling-left">(000) 000-0000</td>

  <td class="t-email t-row-3 t-block t-col-sibling-left">
    <div class="truncate"><a href="#" class="link-secondary link-underline">firstname{{   longFirstName }}lastname{{ longLastName }}@bigtimecompany.com</a>
    </div>
  </td>
{% if contactType == 'customer' %}
  <!-- START - For "Customer List" only -->
  <td class="t-customer-type t-row-3 t-block t-col-sibling-left">
    {% if isOnlineCustomer %}
    <span class="customer-type--label">Online Customer</span>
    <span class="customer-type--icon">
      <span class="lt-icon lt-checkmark"></span>
    </span>
    {% endif %}
  </td>
  <!-- END - For "Customer List" only -->
{% endif %}
  <td class="t-groups t-row-3 t-block t-col-sibling-left">
    {% if groupName %}<span class="label label-secondary label-wrap">{{ groupName }}</span>{% endif %}
    {% if groupName2 %}<span class="label label-secondary label-wrap">{{ groupName2 }}</span>{% endif %}
  </td>
  <td class="t-actions t-row-1 t-self-end">

    <div class="btn-group">
      <a href="javascript:void(0)" data-toggle="dropdown" class="link-secondary"><span class="lt-icon lt-ellipsis lt-large"></span></a>
      <ul class="dropdown-menu pull-right">
        <li><a href="#">Lorem</a></li>
        <li><a href="#">Ipsum</a></li>
        <li><a href="#">Dolor</a></li>
      </ul>
    </div>
  </td>
</tr>
