<tr class="t-has-absolute-col-left-and-right">
  <!-- CHECKBOX -->
  <td class="t-checkbox t-absolute-col-left pr-0">
    {% include "_includes/elements/forms/checkbox.html.twig" with { checked: false, classes: 'input-sm contact-list-checkbox-item ', wrapLabel: true } %}
  </td>
  <!-- FIRST NAME -->
  <td class="t-first-name t-primary t-inline">
    <div class="truncate">
      <a href="#" class="link-secondary link-underline font-weight-bold">Firstname{{ longFirstName }}</a>
    </div>
  </td>
  <!-- LAST NAME -->
  <td class="t-last-name t-primary t-inline">
    <div class="truncate">
      <a href="#" class="link-secondary link-underline font-weight-bold">Lastname{{ longLastName }}</a>
    </div>
  </td>

  {% if contactType == 'team' %}
  <!-- START - "TEAM" COLUMNS -->
  <!-- MENTOR -->
  <td class="t-mentor t-block">
    <div class="truncate">
      <span class="t-key">Mentor: </span>Contact Name{{ longContactName }}
    </div>
  </td>
  <!-- GEN -->
  <td class="t-gen t-block">
    <div class="truncate">
      <span class="t-key">Gen: </span>00000
    </div>
  </td>
  <!-- END - "TEAM" COLUMNS -->
{% endif %}

  <!-- PHONE -->
  <td class="t-phone t-block">(000) 000-0000</td>
  <!-- EMAIL -->
  <td class="t-email t-block">
    <div class="truncate"><a data-toggle="modal" data-target="#modal-contacts-send-email" href="javascript:void(0)" class="link-secondary link-underline">firstname{{ longFirstName }}lastname{{ longLastName }}@bigtimecompany.com</a>
    </div>
  </td>
{% if contactType == 'customer' %}
  <!-- START - "CUSTOMER" COLUMNS -->
  <!-- CUSTOMER -->
  <td class="t-customer-type t-block">
    {% if isOnlineCustomer %}
    <span class="customer-type--label">Online Customer</span>
    <span class="customer-type--icon">
      <span class="lt-icon lt-checkmark lt-medium"></span>
    </span>
    {% endif %}
  </td>
  <!-- END - "CUSTOMER" COLUMNS -->
{% endif %}
