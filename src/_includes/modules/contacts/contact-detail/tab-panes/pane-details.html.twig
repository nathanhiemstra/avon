  {% include '_includes/modules/contacts/contact-detail/tab-panes/details/detail-item.html.twig' with {
    label: 'Sales Tax',
    canEdit: isEditableSalesTax,
    learnMoreTarget: '#change-me',
    content: '<p class="mb-0">0.00% (default)</p>',
    formContent: thisFormContent
  } %}

  <!-- END - SALES TAX -->



  <!-- START - DELIVERY OPTION -->
   <hr>
  {% set thisFormContent %}
    {% include '_includes/modules/contacts/contact-detail/tab-panes/details/delivery-form.html.twig' %}
  {% endset %}

  {% include '_includes/modules/contacts/contact-detail/tab-panes/details/detail-item.html.twig' with {
    label: 'Delivery Option',
    canEdit: isEditableDeliveryOption,
    learnMoreTarget: '#change-me',
    content: '<p class="mb-0">Direct Delivery only (default)</p>',
    formContent: thisFormContent
  } %}
